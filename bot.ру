# –•—Ä–∞–Ω–µ–Ω–∏–µ –æ—Ç–≤–µ—Ç–æ–≤ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π
user_answers = {}

@dp.message_handler(commands=["start"])
async def start(message: types.Message):
    user_id = message.from_user.id
    user_answers[user_id] = []
    
    await message.answer("–ü—Ä–∏–≤–µ—Ç! –î–∞–≤–∞–π –æ–ø—Ä–µ–¥–µ–ª–∏–º —Ç–≤–æ–π –ö–æ–¥ –Ω–∞—Å–ª–∞–∂–¥–µ–Ω–∏—è üåü\n–û—Ç–≤–µ—á–∞–π –Ω–∞ –≤–æ–ø—Ä–æ—Å—ã, –≤—ã–±–∏—Ä–∞—è —Ü–∏—Ñ—Ä—ã.")
    await ask_question(user_id, 0)

async def ask_question(user_id, index):
    if index < len(questions):
        await bot.send_message(user_id, questions[index])
    else:
        profile = analyze_answers(user_answers[user_id])
        await bot.send_message(user_id, f"‚ú® –¢–≤–æ–π –ö–æ–¥ –Ω–∞—Å–ª–∞–∂–¥–µ–Ω–∏—è: {profile} ‚ú®")

def analyze_answers(user_answers):
    scores = {"–≠—Å—Ç–µ—Ç": 0, "–ß—É–≤—Å—Ç–≤—É—é—â–∏–π": 0, "–ò—Å—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å": 0, "–î—É—à–∞ –∫–æ–º–ø–∞–Ω–∏–∏": 0, "–°–æ–∑–¥–∞—Ç–µ–ª—å –±–∞–ª–∞–Ω—Å–∞": 0}
    
    for answer in user_answers:
        if answer == "1":
            scores["–≠—Å—Ç–µ—Ç"] += 1
        elif answer == "2":
            scores["–ß—É–≤—Å—Ç–≤—É—é—â–∏–π"] += 1
        elif answer == "3":
            scores["–ò—Å—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å"] += 1
        elif answer == "4":
            scores["–î—É—à–∞ –∫–æ–º–ø–∞–Ω–∏–∏"] += 1
        elif answer == "5":
            scores["–°–æ–∑–¥–∞—Ç–µ–ª—å –±–∞–ª–∞–Ω—Å–∞"] += 1

    dominant_profile = max(scores, key=scores.get)
    return dominant_profile

@dp.message_handler()
async def handle_answer(message: types.Message):
    user_id = message.from_user.id
    if user_id in user_answers:
        if message.text in ["1", "2", "3", "4", "5"]:
            user_answers[user_id].append(message.text)
            await ask_question(user_id, len(user_answers[user_id]))
        else:
            await message.answer("–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤—ã–±–µ—Ä–∏ —Ü–∏—Ñ—Ä—É 1-5 –¥–ª—è –æ—Ç–≤–µ—Ç–∞ üòâ")

# –ó–∞–ø—É—Å–∫ –±–æ—Ç–∞
if __name__ == "__main__":
    executor.start_polling(dp, skip_updates=True)
